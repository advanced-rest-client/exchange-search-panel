{
  "name": "exchange-search-panel",
  "version": "0.1.0",
  "license": "Apache-2.0 OR CC-BY-4.0",
  "description": "An element that displays an UI to search Anypoint Exchange for RAML (REST API) resources",
  "devDependencies": {
    "bower": "^1.8.0",
    "polymer-cli": "^1.2.0"
  },
  "private": false,
  "main": [
    "exchange-search-panel.html"
  ],
  "author": "The Advanced REST client authors <arc@mulesoft.com>",
  "scripts": {
    "serve": "node_modules/.bin/polymer serve",
    "lint": "node_modules/.bin/polymer lint exchange-search-panel.html exchange-search-grid-item.html exchange-search-list-item.html --rules polymer-1",
    "test-sauce": "node_modules/.bin/polymer test --plugin sauce --job-name \"exchange-search-panel:local-test\"",
    "test": "node_modules/.bin/polymer test --plugin local",
    "deps": "npm install --quiet && bower install --quiet",
    "update-deps": "npm update --quiet && bower update --quiet"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/advanced-rest-client/exchange-search-panel.git"
  },
  "bugs": {
    "url": "https://github.com/advanced-rest-client/exchange-search-panel/issues",
    "email": "arc@mulesoft.com"
  }
}
